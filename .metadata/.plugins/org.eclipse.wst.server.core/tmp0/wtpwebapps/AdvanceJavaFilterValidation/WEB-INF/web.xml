<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <filter>
        <filter-name>FirstFilter</filter-name>
        <filter-class>com.excelr.FirstFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FirstFilter</filter-name>
        <url-pattern>/VoteValidation</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>SecondFilter</filter-name>
        <filter-class>com.excelr.SecondFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>SecondFilter</filter-name>
        <url-pattern>/VoteValidation</url-pattern>
    </filter-mapping>

    <servlet>
        <servlet-name>VoteValidation</servlet-name>
        <servlet-class>com.excelr.VoteValidation</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>VoteValidation</servlet-name>
        <url-pattern>/VoteValidation</url-pattern>
    </servlet-mapping>

</web-app>
