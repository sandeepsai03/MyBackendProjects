  <beans xmlns="http://www.springframework.org/schema/beans"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:context="http://www.springframework.org/schema/context"
          xsi:schemaLocation="
             http://www.springframework.org/schema/beans
             http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
             http://www.springframework.org/schema/context
             http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<!-- Spring bean definition -->
<bean id="std1" class="com.excelr.Student">
<property name="sno">
<value>300</value>
</property>
<property name="smarks">
<list>
<value>67</value>
<value>87</value>
<value>67</value> <!-- list allows duplicates -->
</list>
</property>
<property name="semails">
<set>
<value>abc@gmail.com</value><!-- set doesn't allows duplicates -->
<value>xyz@gmail.com</value>
</set>
</property>
<property name="course">
<map>
<entry key="java" value="50000"></entry>
<entry key="python" value="40000"></entry>
</map>
</property>
</bean>


</beans>