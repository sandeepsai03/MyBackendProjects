  <beans xmlns="http://www.springframework.org/schema/beans"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns:context="http://www.springframework.org/schema/context"
          xsi:schemaLocation="
             http://www.springframework.org/schema/beans
             http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
             http://www.springframework.org/schema/context
             http://www.springframework.org/schema/context/spring-context-3.0.xsd">

<!-- Spring bean definition -->
<bean id="emp" class="com.excelr.Employee">

<!-- assign the values by using <property> tag -->
<!-- 
<property name="empNo">
<value>123</value>
</property>
<property name="empName">
<value>saisandeep</value>
</property>
<property name="empSal">
<value>50000</value>
</property>
</bean> 
-->

<!-- asign the values by using <constructor-arg> tag -->
<constructor-arg name="empNo">
<value>123</value>
</constructor-arg>
<constructor-arg name="empName">
<value>saisandeep</value>
</constructor-arg>
<constructor-arg name="empSal">
<value>50000</value>
</constructor-arg>
</bean>

</beans>